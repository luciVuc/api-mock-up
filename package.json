{
  "name": "api-mock-up",
  "version": "1.0.4",
  "description": "Emulates REST API services by mapping API Request endpoints to predefined API Response values.",
  "main": "dist/index",
  "bin": {
    "api-mock-up": "./bin/api-mock-up"
  },
  "scripts": {
    "prebuild": "npm run clean",
    "build": "tsc",
    "clean": "rimraf ./dist",
    "start:dev": "npm run clean && ts-node-dev --respawn --transpile-only ./src/index.ts -f ./test/demo.json -p 2002",
    "start:debug": "npm run clean && ts-node-dev --inspect-brk --respawn --transpile-only ./src/index.ts -f ./test/demo.json -p 2002",
    "start": "node .",
    "prestart": "npm run build",
    "postinstall": "npm run build",
    "test:dev": "jest --detectOpenHandles ./src/*",
    "test": "npm run build && jest --detectOpenHandles ./dist/*",
    "restart": "npm start",
    "prod": "npm run build && npm run start"
  },
  "keywords": [
    "rest",
    "api",
    "endpoint",
    "endpoints",
    "mock",
    "service",
    "server"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/luciVuc/api-mock-up"
  },
  "author": "Lucian Vuc (https://github.com/luciVuc)",
  "license": "ISC",
  "dependencies": {
    "@types/ajv": "^1.0.0",
    "@types/axios": "^0.14.0",
    "@types/colors": "^1.2.1",
    "@types/cors": "^2.8.13",
    "@types/express": "^4.17.15",
    "@types/jest": "^29.2.5",
    "@types/node": "^18.11.18",
    "@types/uuid": "^9.0.0",
    "@types/yargs": "^17.0.19",
    "ajv": "^8.12.0",
    "axios": "^1.2.2",
    "colors": "^1.4.0",
    "cors": "^2.8.5",
    "dotenv": "^16.0.3",
    "express": "^4.18.2",
    "express-async-handler": "^1.2.0",
    "path": "^0.12.7",
    "rimraf": "^3.0.2",
    "typescript": "^4.9.4",
    "uuid": "^9.0.0",
    "yargs": "^17.6.2"
  },
  "devDependencies": {
    "concurrently": "^7.6.0",
    "jest": "^29.3.1",
    "nodemon": "^2.0.20",
    "ts-jest": "^29.0.3",
    "ts-node": "^10.9.1",
    "ts-node-dev": "^2.0.0"
  }
}
